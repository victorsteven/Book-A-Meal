{
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node ./api/index.js",
    "babel-node": "babel-node index --presets=env",
    "build": "babel -d ./build ./api -s",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "migrate": "npm run sequelize db:migrate",
    "migrate-test": "NODE_ENV=test npm run sequelize db:migrate",
    "seed": "npm run sequelize db:seed:all",
    "seed-test": "NODE_ENV=test npm run sequelize db:seed:all",
    "test": "set NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all  && nyc mocha  --timeout 20000 tests/*.test.js --exit --compilers js:babel-register",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info",
    "test-dev": "NODE_ENV=test nyc mocha --timeout 20000 api/tests/*.test.js --exit --compilers js:babel-register"
  },
  "engines": {
    "node": "10.x"
  },
  "keywords": [],
  "author": "Steven Victor",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "esm": "^3.2.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "swagger-jsdoc": "^3.0.2",
    "path": "^0.12.7",
    "continuation-local-storage": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-core": "^6.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  }
}
